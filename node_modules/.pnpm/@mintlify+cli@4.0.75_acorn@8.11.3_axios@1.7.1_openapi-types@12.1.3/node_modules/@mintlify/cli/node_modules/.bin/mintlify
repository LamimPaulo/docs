#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/paulo/projects/docs/node_modules/.pnpm/@mintlify+cli@4.0.75_acorn@8.11.3_axios@1.7.1_openapi-types@12.1.3/node_modules/@mintlify/cli/bin/node_modules:/home/paulo/projects/docs/node_modules/.pnpm/@mintlify+cli@4.0.75_acorn@8.11.3_axios@1.7.1_openapi-types@12.1.3/node_modules/@mintlify/cli/node_modules:/home/paulo/projects/docs/node_modules/.pnpm/@mintlify+cli@4.0.75_acorn@8.11.3_axios@1.7.1_openapi-types@12.1.3/node_modules/@mintlify/node_modules:/home/paulo/projects/docs/node_modules/.pnpm/@mintlify+cli@4.0.75_acorn@8.11.3_axios@1.7.1_openapi-types@12.1.3/node_modules:/home/paulo/projects/docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/paulo/projects/docs/node_modules/.pnpm/@mintlify+cli@4.0.75_acorn@8.11.3_axios@1.7.1_openapi-types@12.1.3/node_modules/@mintlify/cli/bin/node_modules:/home/paulo/projects/docs/node_modules/.pnpm/@mintlify+cli@4.0.75_acorn@8.11.3_axios@1.7.1_openapi-types@12.1.3/node_modules/@mintlify/cli/node_modules:/home/paulo/projects/docs/node_modules/.pnpm/@mintlify+cli@4.0.75_acorn@8.11.3_axios@1.7.1_openapi-types@12.1.3/node_modules/@mintlify/node_modules:/home/paulo/projects/docs/node_modules/.pnpm/@mintlify+cli@4.0.75_acorn@8.11.3_axios@1.7.1_openapi-types@12.1.3/node_modules:/home/paulo/projects/docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.js" "$@"
else
  exec node  "$basedir/../../bin/index.js" "$@"
fi
