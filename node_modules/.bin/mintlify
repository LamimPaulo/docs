#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/paulo/projects/typebot.io/node_modules/.pnpm/mintlify@4.0.75_acorn@8.11.3_axios@1.6.7_openapi-types@12.1.3/node_modules/mintlify/node_modules:/home/paulo/projects/typebot.io/node_modules/.pnpm/mintlify@4.0.75_acorn@8.11.3_axios@1.6.7_openapi-types@12.1.3/node_modules:/home/paulo/projects/typebot.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/paulo/projects/typebot.io/node_modules/.pnpm/mintlify@4.0.75_acorn@8.11.3_axios@1.6.7_openapi-types@12.1.3/node_modules/mintlify/node_modules:/home/paulo/projects/typebot.io/node_modules/.pnpm/mintlify@4.0.75_acorn@8.11.3_axios@1.6.7_openapi-types@12.1.3/node_modules:/home/paulo/projects/typebot.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mintlify/index.js" "$@"
else
  exec node  "$basedir/../mintlify/index.js" "$@"
fi
